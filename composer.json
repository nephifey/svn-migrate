{
  "name": "nephifey/svn-migrate",
  "type": "library",
  "description": "SVN to Git migration tool that can be used to convert SVN repos to Git repos.",
  "keywords": ["svn", "git", "migrate", "convert"],
  "homepage": "https://github.com/nephifey/svn-migrate",
  "license": "GPL-3.0-or-later",
  "authors": [{
    "name": "Nathan Phifer",
    "email": "nathan@visionary.com",
    "homepage": "https://visionary.com",
    "role": "Developer"
  }],
  "require": {
    "php": ">=7.4",
    "symfony/console": "^5.4",
    "symfony/process": "^5.4",
    "symfony/validator": "^5.4",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.6"
  },
  "autoload": {
    "psr-4": {
      "SvnMigrate\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SvnMigrate\\": "tests"
    }
  },
  "bin": [
    "bin/svn-migrate"
  ]
}
